#!/usr/bin/env python

from freetrain.log import setup_logging

# Configure logging: (needs to be done before importing our modules)
log_conf_locations = ["~/.freetrain/logging.conf", "./logging.conf"]
setup_logging(log_conf_locations)

from logging import getLogger
log = getLogger("freetrain")

if __name__ == "__main__":
    log.debug("Hello world!")
